alias -s zip='unzip'
alias -s rar='unrar x'
alias -s gz='tar -xzvf'
alias -s bz2='tar -xjvf'
alias -s xz='tar -Jxf'
alias -g L='| less'
alias ls='ls -FNh --tabsize=0 --color=auto --show-control-chars --group-directories-first'
alias ls-l='ls -lha'
alias ls-d='ls -lhrt'
alias mv='mv -i'
alias rm='rm -I'
alias du='du -h'
alias diff='colordiff'
alias pacu='sudo pacman -Syu'
alias pacs="pacman -Ss"
alias myip='curl ifconfig.me/ip'
alias mutt='dtach -A $XDG_RUNTIME_DIR/mutt mutt -F $HOME/.config/mutt/muttrc'
