notes() {
    if (($# > 1 )); then
        echo "Invalid number of arguments were given"
    elif (($# == 0)); then
        vim ~/Archives/txt/NOTES
    elif [[ $1 = "-o" ]]; then
        cat ~/Archives/txt/NOTES
    else
        echo "Write notes to edit notes"
        echo "Write notes -o to output notes"
    fi
}
