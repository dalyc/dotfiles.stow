alias cd.dot='cd ~/.dotfiles'
alias cd.notes='cd ~/dev/notes'
alias cd.bin='cd ~/dev/bin'
alias cd.trash='cd ~/.local/share/Trash'

alias bton='systemctl --user start bt.service'
alias btoff='systemctl --user stop bt.service'
alias btstatus='systemctl --user status bt.service'
alias pa-on='systemctl --user start pulse.service'
alias pa-off='systemctl --user stop pulse.service'
alias pa-status='systemctl --user status pulse.service'
