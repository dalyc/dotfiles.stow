alias cd.dot='cd ~/.dotfiles'
alias cd.notes='cd ~/dev/notes'
alias cd.bin='cd ~/dev/bin'
alias cifs='sudo mount -t cifs //eee/dropbox.lec $HOME/dev/UNI -o user=fileserver,uid=shivalva'
alias weechat='dtach -A $XDG_RUNTIME_DIR/weechat weechat --dir ~/.config/weechat'
alias newsbeuter='dtach -A $XDG_RUNTIME_DIR/newsbeuter newsbeuter'
alias bton='systemctl --user start bt.service'
alias btoff='systemctl --user stop bt.service'
alias btstatus='systemctl --user status bt.service'
